package com.app.vacancyportal.entity;

import java.util.Date;


import jakarta.persistence.Column;
import jakarta.persistence.Entity;
import jakarta.persistence.GeneratedValue;
import jakarta.persistence.GenerationType;
import jakarta.persistence.Id;
import jakarta.persistence.Table;
import lombok.Builder;


@Builder
@Entity
@Table(name = "userdetail")
public class UserDetail {

	@Id()
	@GeneratedValue(strategy = GenerationType.IDENTITY)
	@Column(name = "user_id")
	private int userId;
	@Column(name = "first_name")
	private String firstName;
	@Column(name = "last_name")
	private String lastName;
	@Column(name = "selected_profile_pic_id")
	private String selectedPicPath;
	@Column(name = "created_at")
	private Date createdAt;
	@Column(name = "updated_at")
	private Date updatedAt;
	@Column(name = "registration_email")
	private String registrationEmail;
	public int getUserId() {
		return userId;
	}
	public void setUserId(int userId) {
		this.userId = userId;
	}
	public String getFirstName() {
		return firstName;
	}
	public void setFirstName(String firstName) {
		this.firstName = firstName;
	}
	public String getLastName() {
		return lastName;
	}
	public void setLastName(String lastName) {
		this.lastName = lastName;
	}
	public String getSelectedPicPath() {
		return selectedPicPath;
	}
	public void setSelectedPicPath(String selectedPicPath) {
		this.selectedPicPath = selectedPicPath;
	}
	public Date getCreatedAt() {
		return createdAt;
	}
	public void setCreatedAt(Date createdAt) {
		this.createdAt = createdAt;
	}
	public Date getUpdatedAt() {
		return updatedAt;
	}
	public void setUpdatedAt(Date updatedAt) {
		this.updatedAt = updatedAt;
	}
	public String getRegistrationEmail() {
		return registrationEmail;
	}
	public void setRegistrationEmail(String registrationEmail) {
		this.registrationEmail = registrationEmail;
	}
	@Override
	public String toString() {
		return "UserDetail [userId=" + userId + ", firstName=" + firstName + ", lastName=" + lastName
				+ ", selectedPicPath=" + selectedPicPath + ", createdAt=" + createdAt + ", updatedAt=" + updatedAt
				+ ", registrationEmail=" + registrationEmail + "]";
	}
	
	

}
